<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/mine_dialog_padding_left">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_middle"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="@string/mine_order_date"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_order_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="123452362752823425"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="@string/mine_order_num"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_order_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-05-05  14:00:00"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="借用油品："
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="93#"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="单价："
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6.02"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="吨数："
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_liter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="500"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px"

            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="金额（单价*升数）:"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_proce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5000"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px"

            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="借用费："
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_storage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="45000"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px"

            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_padding_big"
        android:layout_marginTop="@dimen/common_padding_big"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_padding_small"
            android:text="转入金额（实际转入金额） ："
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px" />

        <TextView
            android:id="@+id/tv_browoil_into"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="45000"
            android:textColor="@color/common_black_light"
            android:textSize="@dimen/common_bottom_small_22px"

            />

    </LinearLayout>


</LinearLayout>